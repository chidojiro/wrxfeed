{
  "name": "gravity-web",
  "version": "1.0.2",
  "description": "Web UI for Gravity fintech",
  "author": "GravityLabs",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "react-app-rewired start",
    "prebuild": "sh scripts/prebuild.sh",
    "commit-build": "sh scripts/generate-prebuild.sh",
    "build": "yarn prebuild && react-app-rewired build",
    "test": "react-app-rewired test",
    "analyze": "webpack --mode development",
    "preversion": "git-branch-is -r \"^(feat|refactor|chore|fix|hotfix)/[\\w\\-\\d\\.]+$\" && echo Preversion checks passed. Your branch name is correct format.",
    "testw": "jest --watch",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet  --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "increase-version": "sh scripts/version.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn preversion && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{json,md,svg}": [
      "prettier --write"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
